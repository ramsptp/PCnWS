# --- Supabase POOLER Database Connection ---
# 1. Copy this file and rename it to "application.properties"
# 2. Get the real host and password from the team lead.

spring.datasource.url=jdbc:postgresql://YOUR_POOLER_HOST_HERE:6543/postgres
spring.datasource.username=postgres
spring.datasource.password=YOUR_DATABASE_PASSWORD_HERE

# --- JPA / Hibernate Settings ---
spring.jpa.hibernate.ddl-auto=validate
spring.jpa.show-sql=true